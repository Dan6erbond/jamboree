{
  "name": "jamboree",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "yarn download-schema && yarn codegen && next build",
    "start": "next start",
    "lint": "next lint",
    "download-schema": "apollo service:download --endpoint=$JAMBOREE_API_URL graphql-schema.json",
    "codegen": "apollo codegen:generate --localSchemaFile=graphql-schema.json --includes {pages,src}/**/* --target=typescript --tagName=gql --globalTypesFile=src/types/graphql.ts"
  },
  "dependencies": {
    "@apollo/client": "^3.7.0",
    "@emotion/react": "^11.10.4",
    "@emotion/server": "^11.10.0",
    "@fontsource/lobster": "^4.5.11",
    "@mantine/core": "^5.6.0",
    "@mantine/dates": "^5.6.0",
    "@mantine/dropzone": "^5.6.0",
    "@mantine/form": "^5.6.0",
    "@mantine/hooks": "^5.6.0",
    "@mantine/next": "^5.6.0",
    "@mantine/notifications": "^5.6.0",
    "@tabler/icons": "^1.106.0",
    "cookies-next": "^2.1.1",
    "dayjs": "^1.11.5",
    "deepmerge": "^4.2.2",
    "emoji-picker-react": "^4.4.3",
    "lodash": "^4.17.21",
    "next": "12.3.1",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.186",
    "@types/node": "^18.11.3",
    "@types/react": "^18.0.21",
    "@types/react-dom": "18.0.6",
    "apollo": "^2.34.0",
    "eslint": "8.25.0",
    "eslint-config-next": "12.3.1",
    "typescript": "^4.8.4"
  },
  "peerDependencies": {
    "graphql": "^16.6.0"
  }
}
